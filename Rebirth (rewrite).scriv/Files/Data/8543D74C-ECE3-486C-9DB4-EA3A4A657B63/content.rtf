\par\plain {\f1\fs24\b0\i0 Preparing Mana Adaptation process\loch\af1\hich\af1\dbch\af1\uc1\u8230\'85 Adaptation duration 24 hours}
\par\plain {\f1\fs24\b0\i0 Preparing System Introduction\loch\af1\hich\af1\dbch\af1\uc1\u8230\'85 Introduction duration 1 hour}
\par\plain {\f1\fs24\b0\i0 Preparing System Tutorials\loch\af1\hich\af1\dbch\af1\uc1\u8230\'85 Tutorial duration 125 days}
\par\plain {\f1\fs24\b0\i0 Beginning Integration of Planet Sol-3 (Local Designation: Earth)}
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 Please Stand By\loch\af1\hich\af1\dbch\af1\uc1\u8230\'85<!$Scr_Ps::1>}
